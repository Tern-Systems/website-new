<Application x:Class="btmc.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:btmc"
             StartupUri="ui/forms/main/Main.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/src/ui/resources/theme/General.xaml" />
               
                <ResourceDictionary Source="pack://application:,,,/src/ui/resources/theme/Generic.xaml" />

                
                <ResourceDictionary Source="pack://application:,,,/src/ui/resources/toolbox/CTabControl/TabControl.style.xaml" />
                <ResourceDictionary Source="pack://application:,,,/src/ui/resources/toolbox/CTabItem/TabItem.style.xaml" />
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
