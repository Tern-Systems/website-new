{
  "displayName": "Website",
  "rootDir": ".",
  "moduleDirectories": [
    "node_modules/",
    "src/"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx"
  ],
  "moduleNameMapper": {
    "^@/(.+)": "<rootDir>/src/$1"
  },
  "testRegex": ".*\\.spec\\.tsx?$",
  "setupFiles": [
    "<rootDir>/setupTests.js"
  ],
  "transform": {
    "^.+\\.tsx?$": "babel-jest"
  },
  "collectCoverageFrom": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ],
  "coverageDirectory": "coverage/",
  "testEnvironment": "jsdom",
  "verbose": true
}