{
  "name": "mini-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "set NODE_ENV=test&& jest --no-cache --passWithNoTests --detectOpenHandles",
    "test:unit": "npm run test -- --testRegex='.*\\.spec\\.tsx?$'",
    "test:e2e": "npm run test -- --bail=1 --testRegex='.*\\.e2e-spec\\.tsx?$'",
    "test:int": "npm run test -- --bail=1 --testRegex='.*\\.e2e-(int-)?spec\\.tsx?$'",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint",
    "prepare": "husky install",
    "lint:fix": "eslint --fix",
    "pre-commit": "lint-staged --allow-empty",
    "pre-push": "npm run lint src/ && npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@splinetool/runtime": "^1.9.27",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.15",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "next": "^14.2.13",
    "next-qrcode": "^2.5.1",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-player": "^2.16.0",
    "react-share": "^5.1.2",
    "react-svg": "^16.2.0",
    "react-to-pdf": "^1.0.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "^14.2.8",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-refresh": "^0.4.19",
    "husky": "^8.0.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "sharp": "^0.33.5",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.5.4"
  }
}
